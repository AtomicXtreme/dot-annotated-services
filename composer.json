{
  "name": "dotkernel/dot-annotated-services",
  "description": "Library providing auto-DI on services via annotations, without needing to write factories",
  "authors": [
    {
      "name": "Popa Tiberiu",
      "email": "tibi@apidemia.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "annotations",
    "services",
    "factories",
    "container",
    "zf2",
    "zf3",
    "service-manager"
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-servicemanager": "^3.0",
    "doctrine/annotations": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "doctrine/cache": "^1.6",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Dot\\AnnotatedServices\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\AnnotatedServices\\": "tests"
    }
  },
  "suggest": {
    "doctrine/cache": "To cache the result of processing the annotations and speed-up your application"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
